
{% block title %}
    Tous les accounts de Steam-Ish
{% endblock %}

{% block body %}

    <table>
        <thead>
            <tr>
                <th>Nom</th>
                <th>Email</th>
                <th>Nickname</th>
            </tr>
        </thead>
        <tbody>
            {% for account in accounts %}
                <tr>
                    <td>{{ account.name }}</td>
                    <td>{{ account.email }}</td>
                    <td>{{ account.nickname }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>

{% endblock %}
